{
  "dashboard": {
    "id": null,
    "title": "Spring Boot Application Metrics",
    "tags": ["spring-boot", "demo-app"],
    "timezone": "browser",
    "panels": [
      {
        "id": 1,
        "title": "JVM Memory Usage",
        "type": "graph",
        "targets": [
          {
            "expr": "jvm_memory_used_bytes{job=\"demo-app\"}",
            "legendFormat": "{{area}} - Used",
            "refId": "A"
          },
          {
            "expr": "jvm_memory_committed_bytes{job=\"demo-app\"}",
            "legendFormat": "{{area}} - Committed",
            "refId": "B"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 0
        }
      },
      {
        "id": 2,
        "title": "HTTP Request Rate",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(http_server_requests_seconds_count{job=\"demo-app\"}[5m])",
            "legendFormat": "{{method}} {{uri}}",
            "refId": "A"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 0
        }
      },
      {
        "id": 3,
        "title": "HTTP Response Time",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(http_server_requests_seconds_sum{job=\"demo-app\"}[5m]) / rate(http_server_requests_seconds_count{job=\"demo-app\"}[5m])",
            "legendFormat": "{{method}} {{uri}}",
            "refId": "A"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 8
        }
      },
      {
        "id": 4,
        "title": "JVM Threads",
        "type": "graph",
        "targets": [
          {
            "expr": "jvm_threads_live_threads{job=\"demo-app\"}",
            "legendFormat": "Live Threads",
            "refId": "A"
          },
          {
            "expr": "jvm_threads_daemon_threads{job=\"demo-app\"}",
            "legendFormat": "Daemon Threads",
            "refId": "B"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 8
        }
      },
      {
        "id": 5,
        "title": "Application Health",
        "type": "stat",
        "targets": [
          {
            "expr": "up{job=\"demo-app\"}",
            "refId": "A"
          }
        ],
        "gridPos": {
          "h": 4,
          "w": 6,
          "x": 0,
          "y": 16
        }
      },
      {
        "id": 6,
        "title": "HTTP Status Codes",
        "type": "table",
        "targets": [
          {
            "expr": "sum(increase(http_server_requests_seconds_count{job=\"demo-app\"}[5m])) by (status)",
            "legendFormat": "{{status}}",
            "refId": "A"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 20
        }
      }
    ],
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "timepicker": {},
    "templating": {
      "list": []
    },
    "annotations": {
      "list": []
    },
    "refresh": "30s",
    "schemaVersion": 27,
    "version": 0,
    "links": []
  }
}